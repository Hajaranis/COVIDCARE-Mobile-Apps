<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cov_eid">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyA9qE0NbBpKseBtrl7kadeVofMqQ53yr-k"/>
        <activity
            android:name=".WelcomeScreen"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />

        <activity android:name=".Register"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity android:name=".Login"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity android:name=".HealthCareJava"

            android:parentActivityName=".MainActivity"/>

        <activity android:name=".AdditionalResource"

            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ScanCodeActivity"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".HealthFacility"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ui.dashboard.CoughSymptom"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ui.dashboard.FeverSymptom"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ui.dashboard.HeadacheSymptom"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ui.dashboard.WashHand"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ui.dashboard.FaceMask"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ui.dashboard.CrowdedPlace"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ui.profile.ProfileEdit"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".RiskStatus"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".RiskQuestionnaire"
            android:parentActivityName=".RiskStatus"/>

    </application>



    <uses-permission android:name="android.permission.CALL_PHONE"/>

